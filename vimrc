execute pathogen#infect()
set number
syntax enable

"colorscheme spring
"colorscheme jun
"colorscheme molokai
"colorscheme a
if has('gui_running')
	colorscheme a
else
	colorscheme molokai
endif

map <F2> :NERDTreeToggle<CR>

set tabstop=4
set shiftwidth=4
set softtabstop=4
set expandtab
set hlsearch
set incsearch

set writebackup
set nobackup
